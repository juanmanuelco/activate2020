{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.1.2",
        "ajcastro/searchable": "^1.5",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "~2.3",
        "dyrynda/laravel-cascade-soft-deletes": "^4.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "infyomlabs/adminlte-templates": "8.0.x-dev",
        "infyomlabs/laravel-generator": "8.0.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "intervention/image": "^2.5",
        "jlapp/swaggervel": "dev-master",
        "laravel/framework": "^8.0",
        "laravel/scout": "^8.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/annotations": "^8.0",
        "laravelcollective/html": "^6.2",
        "laraveles/spanish": "^1.5",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-permission": "^3.17"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/myNotifications.php",
            "app/Helpers/Routes.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
